{
  "name": "chat2components",
  "version": "0.0.1",
  "description": "Converts chat text marked with entities and intents to json structure.",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/tap --cov test/* --reporter=spec",
    "test-lite": "./node_modules/.bin/tap test/* --reporter=spec",
    "watch-test-lite": "./node_modules/.bin/chokidar --verbose --initial \"**/*.js\" -i \"node_modules\" -c \"npm run test-lite\""
  },
  "keywords": [
    "chatbot",
    "intent",
    "entity",
    "component"
  ],
  "author": "Gagan Bansal",
  "license": "MIT",
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "tap": "^12.0.1"
  },
  "dependencies": {}
}
